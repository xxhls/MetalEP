   "source": [
    "## 读取Excel并将其混合为二进展文件"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "from utils.index import save_cache, load_excel, ROOT\n",
    "\n",
    "\n",
    "DATA = ROOT / \"data\"\n",
    "data_mixed = []\n",
    "for file in DATA.glob(\"*.xlsx\"):\n",
    "    df = load_excel(file)\n",
    "    for row in df[1:]:\n",
    "        data_mixed.append(row[:10])\n",
    "save_cache(data_mixed, \"data_mixed.pkl\")"
   ]
  }
